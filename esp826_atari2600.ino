      tick_cpu = execute(MEMORY[PC], MEMORY[PC + 1], MEMORY[PC + 2]);
      PC = PC & 0b0001111111111111;
      //Serial.println();
    }
    else {
      tick_cpu -- ;
      //Serial.println("NOCPU");
    }
  }
  tia();
}
