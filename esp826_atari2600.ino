      if ((p_X_dis >= 80 && p_X_dis <= 111) && takebit(PF2, (int)((p_X_dis / 4) - 20))) VRAM[p_Y_dis][p_X_dis] = COLUPF ;
    }
    else { //if copy playfield
      if ((p_X_dis >= 0 && p_X_dis <= 15) && takebit(PF0, (int)(3.9 - (p_X_dis / 4)))) VRAM[p_Y_dis][p_X_dis] = COLUPF ;
      if ((p_X_dis >= 80 && p_X_dis <= 95) && takebit(PF0, (int)(3.9 - ((p_X_dis / 4) - 20)))) VRAM[p_Y_dis][p_X_dis] = COLUPF ;

      if ((p_X_dis >= 16 && p_X_dis <= 47) && takebit(PF1, (int)((p_X_dis / 4) - 4))) VRAM[p_Y_dis][p_X_dis] = COLUPF ;
      if ((p_X_dis >= 96 && p_X_dis <= 127) && takebit(PF1, (int)((p_X_dis / 4) - 24))) VRAM[p_Y_dis][p_X_dis] = COLUPF;

      if ((p_X_dis >= 48 && p_X_dis <= 90) && takebit(PF2, (int)((p_X_dis / 4) - 12))) VRAM[p_Y_dis][p_X_dis] = COLUPF ;
      if ((p_X_dis >= 128 && p_X_dis <= 159) && takebit(PF2, (int)((p_X_dis / 4) - 32))) VRAM[p_Y_dis][p_X_dis] = COLUPF ;
    }
    //Serial.print(p_Y);
    //Serial.print(" ");
    //Serial.println(p_X);
  }
}
void setup() {

  Serial.begin(921600);
  Serial.println();
  Serial.println(getCpuFrequencyMhz());

  for (int i = 0 ; i < 8192; i++) {
    MEMORY[i] = 0;
  }
  //Serial.println("Loalding Program...");
  for (int i = 0; i < (sizeof(PROG) / sizeof(PROG[0])); i++) {
    MEMORY[4096 + i] = PROG [i];
    //Serial.print(PROG[i], HEX);
  }

  PC = 4096;
  //Serial.print("Program Loaded, PC initialied to ");
  Serial.println(PC);
  halt_cpu = 0;
  tick_cpu = 0;
  tiaclock = 2;
  Serial.println("NEWLINE");

}

void loop() {
  tiaclock++;
  if (tiaclock == 2) {
    tiaclock = 0;
    if (halt_cpu == 0 && tick_cpu == 0) {
     /* Serial.print("Executing: ");
      Serial.print(MEMORY[PC], HEX);
      Serial.print(", arg1: ");
